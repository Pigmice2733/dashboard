{
  "name": "electron-webpack-quick-start",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-config-calebeby start --electron",
    "compile": "webpack-config-calebeby build --electron",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "dependencies": {
    "preact": "^8.2.7",
    "webpack-config-calebeby": "^1.1.0",
    "wpilib-nt-client": "^1.4.2"
  },
  "devDependencies": {
    "electron": "1.7.11",
    "electron-builder": "^19.55.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
